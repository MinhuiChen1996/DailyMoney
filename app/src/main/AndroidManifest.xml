<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dailymoney">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:icon="@drawable/money"
        android:label="@string/app_name"
        android:roundIcon="@drawable/money"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".about"
            android:label="About"></activity>
        <activity android:name=".backupAndrestrore"
            android:label="Backup And Restore"/>
        <activity android:name=".barchart"/>
        <activity android:name=".piechart"/>
        <activity
            android:name=".search"
            android:label="Search" />
        <activity
            android:name=".Record"
            android:label="Record" />
        <activity android:name=".Register"
            android:label="Register"/>
        <activity android:name=".MainActivity" />
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>